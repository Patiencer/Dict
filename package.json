{
  "name": "dict",
  "version": "0.1.7",
  "description": "a dictionary app",
  "author": {
    "name": "bryht",
    "email": "bryht@live.com"
  },
  "license": "ISC",
  "scripts": {
    "publish": "build -p always",
    "start": "electron ./index.js",
    "dist": "build --win --mac"
  },
  "build": {
    "appId": "Dict",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "bryht",
        "repo": "Dict"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "icon": "build/icon.ico",
      "target": [
        "zip"
      ]
    }
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.5",
    "@types/electron": "^1.6.10",
    "@types/electron-json-storage": "^4.0.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.52.1",
    "electron-builder-squirrel-windows": "^19.52.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "cheerio": "^1.0.0-rc.2",
    "electron-gh-releases": "^2.0.4",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^4.0.2",
    "electron-log": "^2.2.13",
    "electron-updater": "^2.18.2",
    "guesslanguage": "^0.2.0",
    "jquery": "^3.2.1",
    "node-google-translate-skidz": "^1.1.1",
    "request": "^2.83.0",
    "update": "^0.7.4"
  }
}