{
  "name": "dict",
  "version": "0.1.9",
  "description": "a dictionary app",
  "author": {
    "name": "bryht",
    "email": "bryht@live.com"
  },
  "license": "ISC",
  "scripts": {
    "start": "electron ./index.js",
    "distall": "build --win --mac",
    "distwin": "build --win",
    "publish": "build -p always",
    "compile": "tsc && npm install",
    "pack": "yarn compile && electron-builder --dir",
    "dist": "yarn compile && electron-builder",
    "release": "yarn compile && electron-builder"
  },
  "build": {
    "appId": "Dict",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "bryht",
        "repo": "Dict"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.5",
    "@types/debug": "^0.0.30",
    "@types/electron": "^1.6.10",
    "@types/electron-json-storage": "^4.0.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.54.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.0",
    "bootstrap": "^3.3.7",
    "cheerio": "^1.0.0-rc.2",
    "electron-debug": "^1.5.0",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^4.0.2",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.19.1",
    "fs-extra": "^5.0.0",
    "guesslanguage": "^0.2.0",
    "jquery": "^3.2.1",
    "node-google-translate-skidz": "^1.1.1",
    "request": "^2.83.0",
    "update": "^0.7.4"
  }
}
